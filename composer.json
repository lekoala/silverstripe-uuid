{
    "name": "lekoala/silverstripe-uuid",
    "description": "Uuid data extension for SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "uuid",
        "module"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "LeKoala",
            "email": "thomas@lekoala.be"
        }
    ],
    "require": {
        "php": "^8.2",
        "silverstripe/framework": "^6",
        "tuupola/base62": "^2.1",
        "ramsey/uuid": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "installer-name": "uuid"
    },
    "scripts": {
        "lint": "phpcs src/ tests/",
        "lint-clean": "phpcbf src/ tests/",
        "test": "phpunit"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/lekoala/silverstripe-uuid/issues"
    }
}
