{
    "name": "lekoala/silverstripe-uuid",
    "description": "Uuid data extension for SilverStripe",
    "type": "silverstripe-module",
    "keywords": [
        "silverstripe",
        "uuid",
        "module"
    ],
    "license": "MIT",
    "authors": [{
        "name": "LeKoala",
        "email": "thomas@lekoala.be"
    }],
    "require": {
        "php": "~7",
        "silverstripe/framework": "^4",
        "tuupola/base62": "^0.10.0",
        "ramsey/uuid": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "installer-name": "uuid"
    },
    "scripts": {
        "lint": "phpcs code/ tests/",
        "lint-clean": "phpcbf code/ tests/",
        "test": "phpunit -v"
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/lekoala/silverstripe-uuid/issues"
    }
}
